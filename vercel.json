{
  "rewrites": [
    {
      "source": "/api/ferry-schedule",
      "destination": "https://www.sunferry.com.hk//eta/timetable/SunFerry_central_muiwo_timetable_eng.csv"
    },
    {
      "source": "/api/holidays",
      "destination": "https://www.1823.gov.hk/common/ical/en.json"
    },
    {
      "source": "/api/eta/mwce",
      "destination": "https://www.sunferry.com.hk/eta/?route=MWCE"
    },
    {
      "source": "/api/eta/cemw",
      "destination": "https://www.sunferry.com.hk/eta/?route=CEMW"
    }
  ],
  "headers": [
    {
      "source": "/api/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
